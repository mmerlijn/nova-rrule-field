{
    "name": "mmerlijn/nova-rrule-field",
    "description": "A Laravel Nova iCal rrule field.",
    "keywords": [
        "laravel",
        "nova",
        "rrule",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/nova": "^5.0",
        "laravel/nova-devtool": "^1.1"
    },
    "repositories": [
        { "type": "composer", "url": "https://nova.laravel.com" }
    ],
    "autoload": {
        "psr-4": {
            "mmerlijn\\NovaRruleField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mmerlijn\\NovaRruleField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
