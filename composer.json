{
    "name": "mmerlijn/nova-rrule-field",
    "description": "A Laravel Nova iCal rrule field.",
    "keywords": [
        "laravel",
        "nova",
        "rrule",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "mmerlijn\\NovaRruleField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mmerlijn\\NovaRruleField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
